<routes xmlns="http://camel.apache.org/schema/spring">
    <route id="sync-response-processor-scheduler" errorHandlerRef="shutdownErrorHandler">

        <from uri="scheduler:sync-response-processor?initialDelay={{sync-response-processor.initial.delay}}&amp;delay={{sync-response-processor.delay}}" />

        <to uri="direct:sync-response-processor" />
        
    </route>
</routes>
